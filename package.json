{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "tsx src",
    "dev": "tsx watch src",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "migrate:latest": "tsx src/database/migrate/bin",
    "gen:types": "kysely-codegen --dialect sqlite --out-file src/database/types.ts && prettier --write src/database/types.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/better-sqlite3": "^7.6.10",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitest/coverage-v8": "^2.0.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "kysely": "^0.27.3",
    "kysely-codegen": "^0.15.0",
    "prettier": "^3.3.2",
    "restclient": "^0.1.3",
    "supertest": "^7.0.0",
    "tsx": "^4.16.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1",
    "vitest": "^2.0.4"
  },
  "dependencies": {
    "better-sqlite3": "^9.5.0",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "kysely": "^0.27.4",
    "zod": "^3.23.8"
  }
}
